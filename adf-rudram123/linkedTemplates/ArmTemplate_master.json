{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-rudram123"
		},
		"AzureSqlDatabase1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSqlDatabase1'"
		},
		"ProductBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ProductBlobStorage1'"
		},
		"TaxTripData_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'TaxTripData'"
		},
		"ADLSTripData_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLSTripData'"
		},
		"tripdata_ls_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'tripdata_ls'"
		},
		"AzureSqlDatabase1_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "myserver0987.database.windows.net"
		},
		"AzureSqlDatabase1_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "mishra-rd015"
		},
		"AzureSqlDatabase1_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "Rudram"
		},
		"TaxTripData_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "myserver0987.database.windows.net"
		},
		"TaxTripData_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "taxi-data"
		},
		"TaxTripData_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "Rudram"
		},
		"ADLSTripData_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://tripdata6870.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-rudram123_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSqlDatabase1_password": {
						"value": "[parameters('AzureSqlDatabase1_password')]"
					},
					"ProductBlobStorage1_connectionString": {
						"value": "[parameters('ProductBlobStorage1_connectionString')]"
					},
					"TaxTripData_password": {
						"value": "[parameters('TaxTripData_password')]"
					},
					"ADLSTripData_accountKey": {
						"value": "[parameters('ADLSTripData_accountKey')]"
					},
					"tripdata_ls_connectionString": {
						"value": "[parameters('tripdata_ls_connectionString')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_server": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_server')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_database": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_database')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_userName": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_userName')]"
					},
					"TaxTripData_properties_typeProperties_server": {
						"value": "[parameters('TaxTripData_properties_typeProperties_server')]"
					},
					"TaxTripData_properties_typeProperties_database": {
						"value": "[parameters('TaxTripData_properties_typeProperties_database')]"
					},
					"TaxTripData_properties_typeProperties_userName": {
						"value": "[parameters('TaxTripData_properties_typeProperties_userName')]"
					},
					"ADLSTripData_properties_typeProperties_url": {
						"value": "[parameters('ADLSTripData_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}